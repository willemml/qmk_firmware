COMB(enter,   KC_ENT,         KC_SPC, KC_BSPC)
COMB(up,      KC_UP,          KC_F,   KC_S)
COMB(down,    KC_DOWN,        KC_C,   KC_S)
COMB(right,   KC_RGHT,        KC_T,   KC_S)
COMB(left,    KC_LEFT,        KC_R,   KC_S)
COMB(lsqb,    KC_LBRACKET,       KC_U,   KC_Y)
COMB(rsqb,    KC_RBRACKET ,      KC_Y,   KC_SCLN)

SUBS(lparen,    "(",        KC_Q,   KC_W)
SUBS(rparen,    ")",        KC_W,   KC_F)

SUBS(copy, SS_DOWN(X_LGUI)"c"SS_UP(X_LGUI), KC_N, KC_E)
SUBS(paste, SS_DOWN(X_LGUI)"v"SS_UP(X_LGUI), KC_N, KC_I)
SUBS(alfred, SS_DOWN(X_LGUI)SS_TAP(X_SPC)SS_UP(X_LGUI), KC_N, KC_T)
